{
  "compilerOptions": {
		"target": "ES2020",
    "module": "CommonJS",
		"moduleResolution": "node",
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"types": ["node"],
  },
  "include": ["src", "types"],
}
